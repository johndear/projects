<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="操作集"
            visible="true"
            id="org.slave4j.actionSet1">
         <menu
               icon="icons/logo.gif"
               id="org.slave4j.menu1"
               label="slave4j">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="org.slave4j.test.CreateJavaAction"
               enablesFor="1"
               id="org.slave4j.test.CreateJavaAction"
               label="新建java"
               menubarPath="org.slave4j.menu1/group1"
               toolbarPath="group1"
               tooltip="新建java">
         </action>
         <action
               class="org.slave4j.popup.actions.JspCodeGenerationAction"
               enablesFor="1"
               id="org.slave4j.popup.actions.JspCodeGenerationAction"
               label="新建jsp"
               menubarPath="org.slave4j.menu1/group1"
               toolbarPath="group1"
               tooltip="新建jsp">
         </action>
         <action
               class="org.slave4j.popup.actions.CodeGenerationAction"
               enablesFor="1"
               id="org.slave4j.popup.actions.CodeGenerationAction"
               label="自动生成"
               menubarPath="org.slave4j.menu1/group1"
               toolbarPath="group1"
               tooltip="自动生成">
         </action>
          <action
               class="org.slave4j.popup.actions.BuildEnvironmentAction"
               enablesFor="1"
               id="org.slave4j.popup.actions.BuildEnvironmentAction"
               label="搭建spring"
               menubarPath="org.slave4j.menu1/group1"
               toolbarPath="group1"
               tooltip="搭建spring">
         </action>
          <action
               class="org.slave4j.actions.OpenExplorerAction"
               enablesFor="1"
               icon="icons/open_explorer.gif"
               id="org.slave4j.actions.OpenExplorerAction"
               label="打开所在的文件夹"
               menubarPath="org.slave4j.menu1/group1"
               toolbarPath="group1"
               tooltip="打开所在的文件夹">
         </action>
      </actionSet>
   </extension>
   
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="org.slave4j.contribution1"
            objectClass="org.eclipse.jdt.core.IJavaElement">
         <menu
               icon="icons/logo.gif"
               id="org.slave4j.menu2"
               label="slave4j"
               path="additions">
            <separator
                  name="group1">
            </separator>
            <separator
                  name="group2">
            </separator>
         </menu>   
         <action
               class="org.slave4j.popup.actions.BuildEnvironmentAction"
               enablesFor="1"
               icon="icons/build_environment.gif"
               id="org.slave4j.popup.actions.BuildEnvironmentAction"
               label="搭建开发环境"
               menubarPath="org.slave4j.menu2/group1"
               tooltip="搭建开发环境">
         </action>
         <action
               class="org.slave4j.popup.actions.CodeGenerationAction"
               enablesFor="1"
               icon="icons/code_generation.gif"
               id="org.slave4j.popup.actions.CodeGenerationAction"
               label="代码生成"
               menubarPath="org.slave4j.menu2/group2"
               tooltip="代码生成">
         </action>
         <action
               class="org.slave4j.popup.actions.JspCodeGenerationAction"
               enablesFor="1"
               icon="icons/code_generation.gif"
               id="org.slave4j.popup.actions.JspCodeGenerationAction"
               label="新建jsp页面"
               menubarPath="org.slave4j.menu2/group2"
               tooltip="新建jsp页面">
         </action>
      </objectContribution>
   </extension>
   
</plugin>
