<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="sys_user" >

  <resultMap id="BaseResultMap" class="test.org.slave4j.ibatis.instance.SysUser" >
		   	<result column="id" property="id" jdbcType="int" />
		   	<result column="email" property="email" jdbcType="varchar" />
		   	<result column="pwd" property="pwd" jdbcType="varchar" />
		   	<result column="nickname" property="nickname" jdbcType="varchar" />
		   	<result column="state" property="state" jdbcType="int" />
		   	<result column="logincount" property="logincount" jdbcType="int" />
		   	<result column="logintime" property="logintime" jdbcType="datetime" />
		   	<result column="deleted" property="deleted" jdbcType="int" />
		   	<result column="createtime" property="createtime" jdbcType="datetime" />
		   	<result column="updatetime" property="updatetime" jdbcType="datetime" />
		   	<result column="createby" property="createby" jdbcType="int" />
		   	<result column="updateby" property="updateby" jdbcType="int" />
		   	<result column="superadmin" property="superadmin" jdbcType="int" />
  </resultMap>
  
  <select id="selectAll" resultMap="BaseResultMap" parameterClass="test.org.slave4j.ibatis.instance.SysUser" >
    select
			id,
			email,
			pwd,
			nickname,
			state,
			logincount,
			logintime,
			deleted,
			createtime,
			updatetime,
			createby,
			updateby,
			superadmin,
	'0' as row 
    from sys_user
  </select>
  
</sqlMap>