<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

	<extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="操作集"
            visible="true"
            id="org.slave4j.actionSet1">
         <menu
               icon="icons/logo.gif"
               id="org.slave4j.menu1"
               label="slave4j">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="org.slave4j.test.CreateJavaAction"
               enablesFor="1"
               id="org.slave4j.test.CreateJavaAction"
               label="新建java"
               menubarPath="org.slave4j.menu1/group1"
               toolbarPath="group1"
               tooltip="新建java">
         </action>
         <action
               class="org.slave4j.popup.actions.JspCodeGenerationAction"
               enablesFor="1"
               id="org.slave4j.popup.actions.JspCodeGenerationAction"
               label="新建jsp"
               menubarPath="org.slave4j.menu1/group1"
               toolbarPath="group1"
               tooltip="新建jsp">
         </action>
         <action
               class="org.slave4j.popup.actions.CodeGenerationAction"
               enablesFor="1"
               id="org.slave4j.popup.actions.CodeGenerationAction"
               label="自动生成"
               menubarPath="org.slave4j.menu1/group1"
               toolbarPath="group1"
               tooltip="自动生成">
         </action>
          <action
               class="org.slave4j.popup.actions.BuildEnvironmentAction"
               enablesFor="1"
               id="org.slave4j.popup.actions.BuildEnvironmentAction"
               label="搭建spring"
               menubarPath="org.slave4j.menu1/group1"
               toolbarPath="group1"
               tooltip="搭建spring">
         </action>
          <action
               class="org.slave4j.actions.OpenExplorerAction"
               enablesFor="1"
               icon="icons/open_explorer.gif"
               id="org.slave4j.actions.OpenExplorerAction"
               label="打开所在的文件夹"
               menubarPath="org.slave4j.menu1/group1"
               toolbarPath="group1"
               tooltip="打开所在的文件夹">
         </action>
      </actionSet>
   </extension>
   
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="org.slave4j.contribution1"
            objectClass="org.eclipse.jdt.core.IJavaElement">
         <menu
               icon="icons/logo.gif"
               id="org.slave4j.menu2"
               label="slave4j"
               path="additions">
            <separator
                  name="group1">
            </separator>
            <separator
                  name="group2">
            </separator>
         </menu>   
         <action
               class="org.slave4j.popup.actions.BuildEnvironmentAction"
               enablesFor="1"
               icon="icons/build_environment.gif"
               id="org.slave4j.popup.actions.BuildEnvironmentAction"
               label="搭建开发环境"
               menubarPath="org.slave4j.menu2/group1"
               tooltip="搭建开发环境">
         </action>
         <action
               class="org.slave4j.popup.actions.CodeGenerationAction"
               enablesFor="1"
               icon="icons/code_generation.gif"
               id="org.slave4j.popup.actions.CodeGenerationAction"
               label="代码生成"
               menubarPath="org.slave4j.menu2/group2"
               tooltip="代码生成">
         </action>
         <action
               class="org.slave4j.popup.actions.JspCodeGenerationAction"
               enablesFor="1"
               icon="icons/code_generation.gif"
               id="org.slave4j.popup.actions.JspCodeGenerationAction"
               label="新建jsp页面"
               menubarPath="org.slave4j.menu2/group2"
               tooltip="新建jsp页面">
         </action>
      </objectContribution>
   </extension>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Liusu Category"
            id="test1.commands.category">
      </category>
      <command
            name="Liusu Command"
            categoryId="test1.commands.category"
            id="test1.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="test1.commands.sampleCommand"
            class="org.slave4j.SampleHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="test1.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+8"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Liusu1 Menu"
               mnemonic="M"
               id="test1.menus.sampleMenu">
            <command
                  commandId="test1.commands.sampleCommand"
                  mnemonic="S"
                  id="test1.menus.sampleCommand">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="test1.toolbars.sampleToolbar">
            <command
                  commandId="test1.commands.sampleCommand"
                  icon="icons/sample.gif"
                  tooltip="Say hello world"
                  id="test1.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
